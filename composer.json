{
    "name": "bedrockphp/skeleton",
    "description": "Skeleton application for Bedrock mini framework.",
    "keywords": ["framework", "bedrock", "skeleton"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "bedrockphp/framework": "~0.0.1",
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
